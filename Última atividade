      $ senha = $ solicitação -> entrada ( 'senha' );        

return  view ( 'form' , [ 'nome' => $ nome , 'sobrenome' => $ sobrenome , 'genero' => $ genero , 'email' => $ email , 'senha' => $ senha ]);

    }

show de função  pública () {
        return  view ( 'dados' );
    }
}
